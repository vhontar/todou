<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.easycodingstudio.todou">

    <application
            android:name=".TodouApplication"
            android:allowBackup="true"
            android:icon="@drawable/ic_compliant"
            android:label="@string/app_name"
            android:roundIcon="@drawable/ic_compliant"
            android:supportsRtl="true"
            android:theme="@style/Theme.Todou">
        <activity android:name="com.easycodingstudio.todou.ui.TodouActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>